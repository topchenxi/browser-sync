@import 'children_stylus/mixin'

fonts = helvetica, arial, sans-serif

body {
  padding : 50px
  font : 14px/1.4 fonts
}

.warp{
    transition : all 2s
    transform : translateY(-50%)
    transform : rotate(30deg)
    transform : scale(2)
    border-radius : 5px
    box-shadow : 10px 10px 5px #888
}
